<div class="row header-not-signed">
  
  <div class="col-xs-1"></div>
  
  <div class="col-xs-2">
    <span class="logospr"><%= link_to "", root_path %></span>
  </div>
  
  <div class="col-xs-8" align="right">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%= f.email_field :email, placeholder: "email", class: "input-login" %>
      <%= f.password_field :password, autocomplete: "off", placeholder: "password", class: "input-login" %>
    <%= f.submit "Login", class: "btn btn-sm btn-default" %>
    <% end %>
  </div>

  <div class="col-xs-1"></div>
</div>

  <div class="row cats-not-signed">
    <div class="col-xs-1"></div>
    <div class="col-xs-10">
      <div class="col-xs-6 products"><%= link_to "Products", "/how" %></div>
      <div class="col-xs-6 services"><%= link_to "Services", "/how" %></div>
    </div>
    <div class="col-xs-1"></div>
  </div>