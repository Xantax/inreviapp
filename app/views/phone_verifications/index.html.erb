<h1>Listing phone_verifications</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_verifications.each do |phone_verification| %>
      <tr>
        <td><%= phone_verification.number %></td>
        <td><%= phone_verification.user %></td>
        <td><%= link_to 'Show', phone_verification %></td>
        <td><%= link_to 'Edit', edit_phone_verification_path(phone_verification) %></td>
        <td><%= link_to 'Destroy', phone_verification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone verification', new_phone_verification_path %>
