<%= simple_form_for(@promoted_rent) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :tag_list %>
    <%= f.input :price %>
    <%= f.input :image %>
    <%= f.input :quantity %>
    <%= f.association :user %>
    <%= f.association :rent %>
    <%= f.input :clicks %>
    <%= f.input :set_clicks %>
    <%= f.input :sell %>
    <%= f.input :unavailable %>
    <%= f.input :temporal_id %>
    <%= f.input :per_temporal_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
