<%
meta title: "Free Market Works",
description: "Inrevi is a global marketplace. Buy and sell digital products and services.",
keywords: %w(inrevi marketplace digital)
%>

<% if user_signed_in? %>

  <% if current_user.user_identifications.count == 1 %>
    <%= render 'user_verified' %>
  <% else %>
    <%= render 'user_not_verified' %>
  <% end %>

<% else %>
  <%= render 'landing_page' %>
<% end %>